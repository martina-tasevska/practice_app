<ul>
  <% @tasks.each do |task| %>
  <li> <%= "#{task.state.to_s}, #{task.text}" %>
       <%= link_to "delete", destroy_path(:id => task.id, user_id: session[:user_id]), method: :delete %>
       <%= link_to "edit status", edit_user_task_path(:id => task.id, user_id: session[:user_id]), method: :get %>
  </li>
  <% end %>
</ul>
<%= button_to 'Add another task', new_user_task_path(user_id: session[:user_id]), method: :get %>
<%= button_to 'Log out', logout_path, method: :delete %>